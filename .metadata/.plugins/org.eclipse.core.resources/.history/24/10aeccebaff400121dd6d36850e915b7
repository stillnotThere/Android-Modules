package com.acropolis.module.listener.minutes.controller;

import com.acropolis.module.listener.minutes.MinutesModuleActivity;

import android.content.Context;
import android.telephony.TelephonyManager;

public class PhoneState
{

	public void FetchManager()
	{
		TelephonyManager _telMngr = (TelephonyManager)
				MinutesModuleActivity.getAppContext().getSystemService(Context.TELEPHONY_SERVICE);
	}
	
}
