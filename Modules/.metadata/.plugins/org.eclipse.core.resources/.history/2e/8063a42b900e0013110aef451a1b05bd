package com.acropolis.radio.module.global;

import java.text.SimpleDateFormat;
import java.util.Date;

/**
 * @author CPH-iMac
 *
 */
public class Common 
{

	final static String dateFormat = "yyyyMMddHHmm";
	final static SimpleDateFormat sdf = new SimpleDateFormat(dateFormat);
	
	public static String getConvertedDate(int milli)
	{
		String convertedDate = "";
		convertedDate = sdf.format(new Date(milli));
		
		return convertedDate;
	}
	
	
}
