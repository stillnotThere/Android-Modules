package com.app.project.acropolis.monitor.plan;

import com.app.project.acropolis.database.DBAdapter;
import com.app.project.acropolis.database.DBOpenHelper;

/**
 * @author CPH-iMac
 *
 */
public class CheckPlan 
{
	
	long BillDate = 0; //milli
	long Lin = 0;
	long Lout = 0;
	long Lrcv = 0;
	long Lsnt = 0;
	long Ldwn = 0;
	long Lup = 0;
	
	long Rin = 0;
	long Rout = 0;
	long Rrcv = 0;
	long Rsnt = 0;
	long Rdwn = 0;
	long Rup = 0;
	
	private void fetchPlan()
	{
		Lin = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_INCOMING));
		Lout = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_OUTGOING));
		Lrcv = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_RECEIVED));
		Lsnt = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_SENT));
		Ldwn = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_DOWNLOADED));
		Lup = Long.parseLong(DBAdapter.getValue(DBOpenHelper.LOCAL_UPLOADED));
		Rin = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_INCOMING));
		Rout = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_OUTGOING));
		Rrcv = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_RECEIVED));
		Rsnt = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_SENT));
		Rdwn = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_DOWNLOADED));
		Rup = Long.parseLong(DBAdapter.getValue(DBOpenHelper.ROAM_UPLOADED));
	}
	
	public static void compare_set(String recorded)
	{
		
	}
	
	
}
