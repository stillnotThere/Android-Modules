package com.acropolis.module.socket;

import java.io.IOException;
import java.io.OutputStream;
import java.net.Socket;

/**
 * @author CPH-iMac
 *
 */
public class SocketConnector implements Runnable
{
	
	public static final String domain = "http://cphcloud.biz"; 
	public static final int port = 80;
	public static Socket socket=null;
	
	public void run()
	{
		socket = new Socket(domain,port);
		socket.setKeepAlive(true);
		OutputStream output = socket.getOutputStream();
		output.write(out.getBytes());
		socket.setSendBufferSize(out.length());
	}
	
	public void setMsg(String msg) throws IOException
	{
		String out = msg;
		
		
		
	}
	
}
